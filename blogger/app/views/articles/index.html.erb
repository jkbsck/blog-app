<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article) %>
    </li>
  <% end %>
</ul>
<% if logged_in? %>
  <%= link_to "Create a New Article", new_article_path,
      class: 'new_article' %>
<% end %>
<p>
  <br>
  <%= link_to "Tags", tags_path %>
</p>
<p>
  <br>
<%= link_to "Authors", authors_path %>
</p>
